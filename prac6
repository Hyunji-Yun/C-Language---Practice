#include <stdio.h>

int main(void) {
   char origLetter;

   scanf(" %c", &origLetter);

   switch(origLetter){
      
      case 'a':
      case 'A':
      printf("Alpha");
      break;
      
      case 'b':
      case 'B':
      printf("Beta");
      break;
      
      default:
      printf("Unknown");
      break;
   }

   return 0;
}